{
    "name": "itsirv/quvel-core",
    "description": "Core module for QuVel Kit - Provides base functionality, service providers, middleware, and traits.",
    "type": "laravel-module",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Irving Gomez",
            "email": "gomirv.work@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "module",
        "quvel",
        "core"
    ],
    "homepage": "https://github.com/itsirv/quvel-core",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "nwidart/laravel-modules": "^12.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1",
        "mockery/mockery": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Core\\Providers\\CoreServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Core\\": "app/",
            "Modules\\Core\\Database\\Factories\\": "database/factories/",
            "Modules\\Core\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Core\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
